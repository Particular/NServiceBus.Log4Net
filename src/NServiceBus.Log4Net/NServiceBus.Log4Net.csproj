<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;netstandard2.0</TargetFrameworks>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <Description>Log4Net support for NServiceBus</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="log4net" Version="[2.0.8, 3.0.0)" />
    <PackageReference Include="NServiceBus" Version="[8.0.0-alpha.541, 9.0.0)" />
    <PackageReference Include="Fody" Version="6.2.5" PrivateAssets="All" />
    <PackageReference Include="Obsolete.Fody" Version="5.2.1" PrivateAssets="All" />
    <PackageReference Include="Particular.Packaging" Version="0.9.0" PrivateAssets="All" />
  </ItemGroup>

</Project>
